{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@Contracts/*": ["artifacts/contracts/*"],
    },
  },
}


